{
  "plugins": [
    "prettier"
  ],
  "parser": "babel-eslint",
  "extends": [
    "airbnb-base",
    "prettier"
  ],
  "env": {
    "browser": true,
    "es6": true
  },
  "rules": {
    "prettier/prettier": "error",
    "import/no-extraneous-dependencies": 0,
    "import/no-unresolved": [
      2,
      {
        "ignore": [
          "root/"
        ]
      }
    ],
    "class-methods-use-this": 0,
    "prefer-promise-reject-errors": 0
  }
}