<%= f = form_for @changeset, Routes.auth_path(@conn, :create), class: "Login-form" %>
  <div class="Login-inputGroup">
    <%= label f, :username %>
    <%= text_input f, :username %>
    <%= error_tag f, :username %>
  </div>

  <div class="Login-inputGroup">
    <%= label f, :password %>
    <%= text_input f, :password, type: :password %>
    <%= error_tag f, :password %>
  </div>

  <div>
    <%= submit "Save" %>
  </div>
</form>
