{
  "extends": ["airbnb", "prettier"],
  "plugins": ["prettier"],
  "parser": "babel-eslint",
  "rules": {
    "prettier/prettier": "error",
    "indent": [
      "error",
      2
    ],
    "linebreak-style": [
      "error",
      "unix"
    ],
    "quotes": [
      "error",
      "double"
    ],
    "semi": [
      "error",
      "always"
    ],
    "no-use-before-define": "off",
    "consistent-return": "off",
    "no-return-assign": "off",
    "no-plusplus": "off",
    "no-bitwise": "off",
    "prefer-destructuring": "off"
  }
}
